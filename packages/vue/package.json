{
  "name": "@silver-formily/vue",
  "type": "module",
  "version": "2.2.1",
  "description": "Vue3 版本的 @formily/vue",
  "author": "hezhengxu",
  "license": "MIT",
  "homepage": "https://vue.silver-formily.org",
  "repository": {
    "type": "github",
    "url": "https://github.com/hezhengxu2018/silver-formily",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/hezhengxu2018/silver-formily/issues"
  },
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.mjs",
      "default": "./esm/index.mjs"
    },
    "./*": "./esm/*"
  },
  "main": "./esm/index.mjs",
  "types": "./esm/index.d.ts",
  "files": [
    "esm"
  ],
  "scripts": {
    "build": "vite build",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "docs:dev": "pnpm --filter vue-docs dev",
    "docs:build": "pnpm --filter vue-docs build",
    "commit": "czg",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@formily/core": "^2",
    "@formily/json-schema": "^2",
    "@formily/reactive": "^2",
    "@formily/shared": "^2",
    "@silver-formily/reactive-vue": "^1.0.0",
    "vue": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@formily/validator": "^2.3.7",
    "@silver-formily/reactive-vue": "workspace:*",
    "@silver-formily/typescript-config": "workspace:*",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/shared": "^3.5.27",
    "commitlint": "^19.8.1",
    "cz-git": "^1.12.0",
    "czg": "^1.12.0",
    "eslint": "^9.33.0",
    "eslint-plugin-format": "^1.3.1",
    "fast-glob": "^3.3.3",
    "typescript": "^5.9.2",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^7.3.1"
  }
}
